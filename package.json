{
  "name": "nodejs-sequelize-architecture",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "migration:run": "npx sequelize-cli db:migrate",
    "migration:undo": "npx sequelize-cli db:migrate:undo",
    "migration:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "migration:create": "npx sequelize-cli migration:generate --name",
    "seeder:create": "npx sequelize-cli seed:generate --name",
    "seeder:run": "npx sequelize-cli db:seed --seed",
    "seeder:all": "npx sequelize-cli db:seed:all",
    "seeder:undo": "npx sequelize-cli db:seed:undo"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.337.0",
    "@faker-js/faker": "^7.6.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "firebase-admin": "^11.5.0",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "md5": "^2.3.0",
    "moment": "^2.29.4",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.9.4",
    "otp-generator": "^4.0.1",
    "redis": "^4.6.8",
    "sequelize": "^6.21.6",
    "sharp": "^0.32.4",
    "winston": "^3.10.0",
    "xmlbuilder": "^15.1.1"
  }
}
